{
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"],
      "target": "ES2019",
    "module": "commonjs",
    "rootDir": "./app/api",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./build",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
    }
  }
}